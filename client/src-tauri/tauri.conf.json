{
  "$schema": "../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Transcriber",
  "version": "0.1.0",
  "identifier": "com.transcriber.ios",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "Transcriber",
        "width": 800,
        "height": 600,
        "minWidth": 320,
        "minHeight": 480,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "developmentTeam": "PLACEHOLDER"
    }
  }
}
